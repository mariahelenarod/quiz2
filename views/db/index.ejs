<div class="panel panel-default table-responsive">
  <div class="panel-heading">Tabla Preguntas</div>
    <table class="table table-hover">
      <thead>
        <tr>
          <th>#</th>
          <th>Tema</th>
          <th>Pregunta</th>
          <th>Respuesta</th>
          <th>Fecha Creación</th>
          <th>Fecha Actualización</th>
        </tr>
      </thead>
      <tbody>
  		  <% var i; for (i=0; i < results.length; i++) {%>
         	<tr>
     		    <td><%=results[i].id%></td>
     		    <td><%=results[i].tema%></td>
     		    <td><a href="/quizes/<%=results[i].id %>"><%=results[i].pregunta%></a></td>
     		    <td><%=results[i].respuesta%></td>
     		    <td><%=results[i].createdAt%></td>
     		    <td><%=results[i].updatedAt%></td>
 		      </tr>
  		  <%}%>
  	 <tbody>
    </table>
</div>